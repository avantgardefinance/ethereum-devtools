{
  "name": "@enzymefinance/example",
  "version": "0.0.0",
  "private": true,
  "main": "dist/enzymefinance-example.cjs.js",
  "module": "dist/enzymefinance-example.esm.js",
  "scripts": {
    "compile": "hardhat compile",
    "typecheck": "tsc --noEmit",
    "lint": "yarn lint:prettier && yarn lint:solhint",
    "lint:eslint": "eslint \"{*,**/*}.{js,jsx,ts,tsx}\"",
    "lint:prettier": "prettier --list-different \"{*,**/*}.{sol,json}\"",
    "lint:solhint": "solhint --max-warnings 0 \"contracts/{*,**/*}.sol\"",
    "format": "yarn format:prettier && yarn format:eslint",
    "format:prettier": "yarn lint:prettier --write",
    "format:eslint": "yarn lint:eslint --fix",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@enzymefinance/ethers": "1.0.11",
    "ethers": "^5.5.3"
  },
  "devDependencies": {
    "@enzymefinance/codegen": "^1.0.1",
    "@enzymefinance/eslint-config": "^1.0.17",
    "@enzymefinance/hardhat": "^1.0.12",
    "@enzymefinance/prettier-config-solidity": "^1.0.1",
    "@openzeppelin/contracts": "^4.3.2",
    "@types/jest": "^27.4.0",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "solhint": "^3.3.4",
    "typescript": "^4.5.5"
  },
  "eslintConfig": {
    "extends": "@enzymefinance/eslint-config",
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    }
  }
}
