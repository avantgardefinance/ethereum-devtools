{
  "name": "@enzymefinance/example",
  "version": "0.0.0",
  "private": true,
  "main": "dist/enzymefinance-example.cjs.js",
  "module": "dist/enzymefinance-example.esm.js",
  "scripts": {
    "compile": "hardhat compile",
    "typecheck": "tsc --noEmit",
    "lint": "yarn lint:prettier && yarn lint:solhint",
    "lint:eslint": "eslint \"{*,**/*}.{js,jsx,ts,tsx}\"",
    "lint:prettier": "prettier --list-different \"{*,**/*}.{sol,json}\"",
    "lint:solhint": "solhint --max-warnings 0 \"contracts/{*,**/*}.sol\"",
    "format": "yarn format:prettier && yarn format:eslint",
    "format:prettier": "yarn lint:prettier --write",
    "format:eslint": "yarn lint:eslint --fix",
    "test": "TS_JEST_DISABLE_VER_CHECKER=true jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@enzymefinance/ethers": "1.0.9",
    "ethers": "^5.5.1"
  },
  "devDependencies": {
    "@enzymefinance/babel-config": "^1.0.16",
    "@enzymefinance/codegen": "^1.0.0",
    "@enzymefinance/eslint-config": "^1.0.14",
    "@enzymefinance/hardhat": "^1.0.9",
    "@enzymefinance/prettier-config-solidity": "^1.0.0",
    "@openzeppelin/contracts": "^4.3.2",
    "@types/jest": "^27.0.2",
    "eslint": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "solhint": "^3.3.4",
    "typescript": "^4.4.4"
  },
  "prettier": "@enzymefinance/prettier-config-solidity",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.json": "prettier --write",
    "*.sol": [
      "prettier --write",
      "solhint --fix"
    ]
  }
}
